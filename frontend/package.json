  "scripts": {
    "delDist": "ts-node --project ./utils/tsconfig.utils.json ./utils/delDistDir.ts ./dist",
    "ngBuild": "ng build",
    "ngBuild:prod": "ng build --prod",
    "prettier": "prettier --write \"./**/*.{ts,tsx}\"",
    "tslint:app": "tslint ./src/**/*.ts --format verbose --project ./src/tsconfig.app.json --config ./src/tslint.json",
    "tslint:spec": "tslint ./src/**/spec.ts --format verbose --project ./src/tsconfig.spec.json --config ./src/tslint.json",
    "tslint": "npm-run-all tslint:app tslint:spec",
    "build:dev": "npm-run-all delDist ngBuild prettier tslint",
    "build:prod": "npm-run-all delDist ngBuild:prod prettier tslint:app",
    "startServer": "cd ../backend && npm run startServer",
    "ngServe": "sleepms 1000 && ng serve --open=false --proxy-config proxy.conf.json",
    "start": "npm-run-all -p -r startServer ngServe",
    "test": "ng test --code-coverage",
    "ngE2e": "sleepms 1000 && ng e2e",
    "e2e": "npm-run-all -p -r startServer ngE2e"
  },
