    "tscServerWatch": "tsc --watch --project ./tsconfig.json",
    "tscClient": "tsc --project ./server/test/client/tsconfig.client.json",
    "tscClientWatch": "tsc --watch --project ./server/test/client/tsconfig.client.json",
    "compile": "npm run delDist && npm run tscServer && npm run tscClient",
    "prettierBase": "prettier",
    "prettierCheck": "npm run prettierBase -- --list-different \"./**/*.{ts,tsx}\"",
    "prettierWrite": "npm run prettierBase -- --write \"./**/*.{ts,tsx}\"",
    "tslintServer": "tslint --format verbose --project ./tsconfig.json --config ./tslint.json",
    "tslintClient": "tslint --format verbose --project ./server/test/client/tsconfig.client.json --config ./tslint.json",
    "tslint": "npm run tslintServer && npm run tslintClient",
    "build": "npm run compile && npm run prettierWrite && npm run tslint && npm run copy"
  },
