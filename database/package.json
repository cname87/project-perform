  ],
  "author": "Se√°n Young",
  "license": "MIT",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node ./dist/database/src/index.js",
    "delDist": "ts-node ../utils-build/delDistDir.ts ./database/dist",
    "tsc": "tsc --project ./",
    "compile": "npm run delDist && npm run tsc",
    "tslint": "tslint --config ./tslint.json --project ./tsconfig.json",
    "prettierBase": "prettier",
    "prettierCheck": "npm run prettierBase -- --list-different \"./**/*.{ts,tsx}\"",
    "prettierWrite": "npm run prettierBase -- --write \"./**/*.{ts,tsx}\"",
    "build": "npm run compile && npm run prettierWrite && npm run tslint"
  },
  "dependencies": {
    "connect-mongodb-session": "^2.0.8",
    "debug": "^4.1.1",
    "express-session": "^1.15.6",
    "mongoose": "^5.4.9",
    "node": "^11.9.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/connect-mongodb-session": "0.0.0",
    "@types/debug": "0.0.31",
    "@types/express-session": "^1.15.11",
    "@types/mongodb": "^3.1.19",
    "@types/mongoose": "^5.3.12",
    "@types/node": "^10.12.21",
    "mongodb": "^3.1.13",
    "prettier": "^1.16.4",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1"
  }
